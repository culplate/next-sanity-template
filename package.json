{
  "name": "next-sanity-template",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:studio": "pnpm --filter studio dev",
    "dev:all": "pnpm --parallel dev dev:studio",
    "build": "turbo run build",
    "build:studio": "pnpm --filter studio build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "typecheck": "turbo run typecheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "turbo": "^2.7.2",
    "prettier": "^3.7.4",
    "prettier-plugin-sort-imports": "^1.8.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "eslint": "^8.57.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
